{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@gluestack/test-performance": ["../.."],
      "@gluestack/popper": ["../../popper/src/index"],
      "@gluestack/ui": ["./../ui/src/index"],
      "@gluestack/ui-styled": ["../../styled/src/index"],
      "@gluestack/color-mode": ["../../color-mode/src/index"]
    },
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "allowJs": true,
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true
  },
  "extends": "expo/tsconfig.base"
}
